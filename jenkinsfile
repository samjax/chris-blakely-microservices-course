pipeline{
    agent any
    tools{
        maven 'Maven_3.9.9'
    }
    stages{
        stage("Clean the project"){
            steps{
                sh '''
                echo "Clean project"
                cd patient-service
                mvn clean
                '''
            }
        }
        stage("Build the project"){
            steps{
                sh '''
                echo "Building the project"
                mvn build
                '''
            }
        }
    }
}
